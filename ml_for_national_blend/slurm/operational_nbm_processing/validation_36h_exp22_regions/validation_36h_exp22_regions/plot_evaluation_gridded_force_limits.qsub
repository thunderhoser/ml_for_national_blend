    --max_colour_values 16 16  23  23 3.30 1.3 10.5 1.00 165 1.00 1.00 1.00 2.5 1.00 +1.2 1.0 +1 1.00 \
    --output_dir_name="${PREDICTION_DIR_NAME}/${region_name}/validation/gridded_evaluation_${target_field_name}"
    
elif [ "$target_field_name" = "wind_gust_10m_agl_m_s01" ]; then
    python3 -u "${CODE_DIR_NAME}/plot_gridded_evaluation.py" \
    --input_eval_file_name_or_pattern="${PREDICTION_DIR_NAME}/${region_name}/validation/gridded_evaluation_${target_field_name}.nc" \
    --target_field_name="${target_field_name}" \
    --metric_names "target_standard_deviation" "prediction_standard_deviation" "target_mean" "prediction_mean" "root_mean_squared_error" "mse_bias" "mse_variance" "mse_skill_score" "dual_weighted_mean_squared_error" "dwmse_skill_score" "kolmogorov_smirnov_statistic" "kolmogorov_smirnov_p_value" "mean_absolute_error" "mae_skill_score" "bias" "correlation" "kling_gupta_efficiency" "reliability" \
    --min_colour_values  1.0  1.0  3  3 1.00 0.00  0.0 0.05   0 0.10 0.00 0.00 0.75 -0.10 -1.8 0.45 -1 0.00 \
    --max_colour_values  5.5  5.5 11 11 3.75 3.75 13.0 1.00 190 1.00 1.00 1.00 3.25  1.00 +1.8 1.00 +1 1.00 \
    --output_dir_name="${PREDICTION_DIR_NAME}/${region_name}/validation/gridded_evaluation_${target_field_name}"

else
    python3 -u "${CODE_DIR_NAME}/plot_gridded_evaluation.py" \
    --input_eval_file_name_or_pattern="${PREDICTION_DIR_NAME}/${region_name}/validation/gridded_evaluation_${target_field_name}.nc" \
    --target_field_name="${target_field_name}" \
    --metric_names "target_standard_deviation" "prediction_standard_deviation" "target_mean" "prediction_mean" "root_mean_squared_error" "mse_bias" "mse_variance" "mse_skill_score" "dual_weighted_mean_squared_error" "dwmse_skill_score" "kolmogorov_smirnov_statistic" "kolmogorov_smirnov_p_value" "mean_absolute_error" "mae_skill_score" "bias" "correlation" "kling_gupta_efficiency" "reliability" \
